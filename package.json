{
  "name": "react-starter",
  "version": "0.1.0",
  "main": "backend/server.js",
  "engines": {
    "node": "4.2.2",
    "npm": "2.14.7"
  },
  "scripts": {
    "lint": "eslint .",
    "build": "source .production; webpack --config webpack.config-prod.js --progress --profile --colors",
    "dev": "source .development; webpack-dev-server --config webpack.config-dev.js --progress --colors --port 2992 --inline",
    "node": "source .production; babel-node backend/server.js",
    "nodemon": "source .development; nodemon backend/server.js",
    "release-patch": "git checkout master && npm version patch && git push origin master --tags",
    "release-minor": "git checkout master && npm version minor && git push origin master --tags",
    "release-major": "git checkout master && npm version major && git push origin master --tags",
    "release-as-lib": "git checkout master && npm run build && git commit -am 'Distribute' && npm version major && git push origin master --tags",
    "mocha": "source .testing; mocha --opts specs/mocha.opts specs",
    "test": "npm run mocha"
  },
  "dependencies": {
    "autoprefixer-loader": "^3.1.0",
    "axios": "^0.15.3",
    "babel": "^6.5.2",
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "baobab": "^2.0.0",
    "baobab-react": "^2.1.2",
    "body-parser": "^1.12.4",
    "bootstrap": "^3.3.4",
    "bootstrap-ext": "^0.2.0",
    "classnames": "^2.1.5",
    "compression": "^1.6.0",
    "cookie-parser": "^1.3.4",
    "css-loader": "^0.26.1",
    "deep-merge": "^1.0.0",
    "emailjs": "^1.0.0",
    "event-stream": "^3.3.1",
    "express": "^4.12.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "faker": "^3.0.1",
    "file-loader": "^0.9.0",
    "flat": "^2.0.1",
    "font-awesome": "^4.3.0",
    "glob": "^7.1.1",
    "globalize": "^1.1.0-rc.2",
    "globalize-webpack-plugin": "^0.3.8",
    "html-loader": "^0.4.4",
    "js-base64": "^2.1.8",
    "json-loader": "^0.5.2",
    "json5": "^0.5.1",
    "json5-loader": "^0.6.0",
    "less": "^2.5.1",
    "less-loader": "^2.2.0",
    "lodash.debounce": "^4.0.8",
    "lodash.throttle": "^4.1.1",
    "marked": "^0.3.3",
    "mkdirp": "^0.5.0",
    "moment": "^2.10.3",
    "morgan": "^1.5.3",
    "node-uuid": "^1.4.3",
    "nunjucks": "^3.0.0",
    "paqmind.data-lens": "^0.6.0",
    "paqmind.tcomb-lens": "^0.2.0",
    "ramda": "^0.22.1",
    "raw-loader": "^0.5.1",
    "react": "^15.4.1",
    "react-document-title": "^2.0.1",
    "react-hot-loader": "^1.2.7",
    "react-proxy-loader": "^0.3.4",
    "react-router": "^3.0.0",
    "readable-stream": "^2.2.2",
    "style-loader": "^0.13.1",
    "tcomb": "^3.2.15",
    "tcomb-validation": "^3.3.0",
    "url-loader": "^0.5.6",
    "util": "^0.10.3",
    "webpack": "^1.12.11",
    "webpack-dev-server": "^1.14.1",
    "winston": "^2.3.0",
    "winston-mail": "^1.3.0",
    "yargs": "^6.5.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "chai": "^3.3.0",
    "cldr-data": "^30.0.2",
    "eslint": "^3.12.2",
    "mocha": "^3.2.0",
    "nodemon": "^1.3.7"
  }
}
